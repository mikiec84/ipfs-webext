{
	"key":"",
  "description": "The Ipfs web extension",
  "manifest_version": 2,
  "name": "IPFS",
  "version": "1.0",
  "icons": {
    "48": "icons/ipfs-48.png",
    "128": "icons/ipfs-128.png"
  },

  "applications": {
    "gecko": {
      "id": "ipfs@ipfs.io",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/ipfs-logo-vector-ice.svg"
  },
 
  "permissions": [
	  "nativeMessaging",
	  "webRequest",
	  "webRequestBlocking",
	  "tabs",
	  "*://*.ipfs.io/",
	  "*://ipfs.io/",
	  "https://*/"
  ]
}
